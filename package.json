    "test": "npx ava",
    "lint": "npx eslint . --ext .js",
    "format": "npm run prettier:fix && npm run lint",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "postinstall": "echo 'VeniceCLI dependencies installed, run `npx zli configure.js`'"
