  "contributors": [
    {
      "name": "Dana Janezic",
      "email": "danajanezic@gmail.com",
      "url": "https://github.com/danajanezic"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/qandamedia/zli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/qandamedia/zli.git"
  },
  "bugs": {
    "url": "https://github.com/qandamedia/zli/issues",
    "email": "team-eng+zli-bugs@venicemusic.co"
  },
  "bin": {
    "zli": "./bin/index.js"
  },
  "devDependencies": {
    "ava": "^5.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-better-mutation": "^1.4.0",
    "eslint-plugin-editorconfig": "^4.0.2",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^39.0.0",
    "prettier": "^2.8.8"
  },
  "type": "module",
  "dependencies": {
    "assign-deep": "^1.0.1",
    "code-stringify": "^2.0.3",
    "commander": "^8.2.0",
    "deep-object-diff": "^1.1.7",
    "enquirer": "^2.3.6",
    "escodegen": "^2.0.0",
    "estree-toolkit": "^1.3.1",
    "i": "^0.3.7",
    "meriyah": "^4.3.0",
    "semver": "^7.3.8",
    "zx": "8.5.5"
  },
  "engines": {
    "node": ">=19.9.0"
